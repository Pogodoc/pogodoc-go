{
  "name": "pogodoc",
  "version": "0.0.4",
  "description": "Pogodoc SDK for TypeScript",
  "keywords": [
    "pogodoc",
    "sdk",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "github.com/Pogodoc/pogodoc-ts"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "tsx src/test.ts",
    "install-imports": "node ./install-imports.js",
    "build": "rimraf dist && node build.js && tsc && copyfiles package.json dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.15.12",
    "@types/qs": "^6.9.18",
    "@types/readable-stream": "^4.0.18",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.25.4",
    "dotenv": "^16.5.0",
    "tsx": "^4.19.4",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "js-base64": "^3.7.7",
    "node-fetch": "^3.3.2",
    "qs": "^6.14.0",
    "readable-stream": "^4.7.0",
    "url-join": "^5.0.0"
  }
}
